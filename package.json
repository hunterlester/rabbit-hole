  },
  "homepage": "https://github.com/hunterlester/rabbit-hole#readme",
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.6",
    "debug": "^2.2.0",
